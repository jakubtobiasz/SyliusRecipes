{
    "manifests": {
        "sylius/return-plugin": {
            "manifest": {
                "bundles": {
                    "Sylius\\ReturnPlugin\\SyliusReturnPlugin": [
                        "all"
                    ]
                },
                "copy-from-recipe": {
                    "config/": "%CONFIG_DIR%/"
                }
            },
            "files": {
                "config/packages/sylius_return_plugin.yaml": {
                    "contents": [
                        "imports:",
                        "    - { resource: \"@SyliusReturnPlugin/config/config.yaml\" }",
                        "",
                        "sylius_return:",
                        "    pdf_generator:",
                        "        enabled: true",
                        ""
                    ],
                    "executable": false
                },
                "config/routes/sylius_return_plugin.yaml": {
                    "contents": [
                        "# Remove if you don't use SyliusAdminBundle",
                        "sylius_return_admin:",
                        "    resource: \"@SyliusReturnPlugin/config/routes/admin_routing.yaml\"",
                        "    prefix: '/%sylius_admin.path_name%'",
                        "",
                        "# Remove if you don't use SyliusShopBundle",
                        "sylius_return_shop:",
                        "    resource: \"@SyliusReturnPlugin/config/routes/shop_routing.yaml\"",
                        "    prefix: /{_locale}",
                        "    requirements:",
                        "        _locale: ^[a-z]{2}(?:_[A-Z]{2})?$",
                        ""
                    ],
                    "executable": false
                }
            },
            "ref": "6c0f3bceb6f7d5c163af3030caaded4b68822dd1"
        }
    }
}

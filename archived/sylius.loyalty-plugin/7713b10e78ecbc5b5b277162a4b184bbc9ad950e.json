{
    "manifests": {
        "sylius/loyalty-plugin": {
            "manifest": {
                "bundles": {
                    "Sylius\\LoyaltyPlugin\\SyliusLoyaltyPlugin": [
                        "all"
                    ]
                },
                "copy-from-recipe": {
                    "config/": "%CONFIG_DIR%/"
                }
            },
            "files": {
                "config/packages/sylius_loyalty_plugin.yaml": {
                    "contents": [
                        "imports:",
                        "    - { resource: \"@SyliusLoyaltyPlugin/config/config.yaml\" }",
                        ""
                    ],
                    "executable": false
                },
                "config/routes/sylius_loyalty_plugin_admin.yaml": {
                    "contents": [
                        "# Remove if you don't use SyliusAdminBundle",
                        "sylius_loyalty_admin:",
                        "    resource: \"@SyliusLoyaltyPlugin/config/admin_routing.yaml\"",
                        "    prefix: '/%sylius_admin.path_name%'",
                        ""
                    ],
                    "executable": false
                },
                "config/routes/sylius_loyalty_plugin_shop.yaml": {
                    "contents": [
                        "# Remove if you don't use SyliusShopBundle",
                        "sylius_loyalty_shop:",
                        "    resource: \"@SyliusLoyaltyPlugin/config/shop_routing.yaml\"",
                        "    prefix: /{_locale}",
                        "    requirements:",
                        "        _locale: ^[a-z]{2}(?:_[A-Z]{2})?$",
                        ""
                    ],
                    "executable": false
                }
            },
            "ref": "7713b10e78ecbc5b5b277162a4b184bbc9ad950e"
        }
    }
}
